<%= link_to :Add, new_user_path, class: "btn btn-primary my-2" %>
<table class="table">
  <thead>
  <tr>
    <th scope="col">#</th>
    <th scope="col">Email</th>
    <th scope="col">Created at</th>
    <th scope="col">Updated at</th>
  </tr>
  </thead>
  <tbody>
  <% @interactor.users.each_with_index do |user| %>
  <tr>
    <td><%= user.id %></td>
    <td><%= link_to  user.email, edit_user_path(user) %></td>
    <td><%= user.created_at %></td>
    <td><%= user.updated_at %></td>
  </tr>
    <% end  %>
  </tbody>
</table>